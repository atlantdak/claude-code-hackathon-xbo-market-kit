includes:
	- vendor/szepeviktor/phpstan-wordpress/extension.neon

parameters:
	level: 6
	paths:
		- xbo-market-kit.php
		- includes/
	scanDirectories:
		- ../../..
	excludePaths:
		- vendor/
		- includes/Blocks/*/render.php
	bootstrapFiles:
		- xbo-market-kit.php
	ignoreErrors:
		- identifier: missingType.iterableValue
		-
			message: '#on an unknown class WP_CLI#'
			identifier: class.notFound
